{
    "name": "vscode-inputstream",
    "displayName": "vscode-inputstream",
    "description": "Weightless developer publishing",
    "version": "0.5.0",
    "publisher": "stackbuild",
    "license": "Apache-2.0",
    "icon": "stackb-full.png",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/inputstream/vscode-inputstream.git"
    },
    "keywords": [
        "blogging",
        "blog",
        "publishing",
        "writing"
    ],
    "author": "stack.build authors",
    "bugs": {
        "url": "https://github.com/inputstream/vscode-inputstream/issues"
    },
    "homepage": "https://github.com/inputstream/vscode-inputstream#readme",
    "categories": [],
    "activationEvents": [
        "onUri",
        "onFileSystem:psfs",
        "onView:input.stream.inputExplorer",
        "onView:input.stream.fileExplorer",
        "onCommand:input.stream.login",
        "onCommand:input.stream.image.search"
    ],
    "engines": {
        "vscode": "^1.45.0"
    },
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "inputstream",
            "properties": {
                "input.stream.enabled": {
                    "type": "boolean",
                    "default": true,
                    "description": "Flag to enable/disable this feature"
                },
                "input.stream.verbose": {
                    "type": "number",
                    "default": 0,
                    "description": "The verbosity level for extra diagnostics"
                },
                "input.stream.api.address": {
                    "type": "string",
                    "default": "api.input.stream:443",
                    "description": "Address of the API server"
                },
                "input.stream.baseDir": {
                    "type": "string",
                    "default": "~/.inputstream",
                    "description": "Base directory to stage input files"
                }
            }
        },
        "commands": [
            {
                "category": "inputstream",
                "command": "input.stream.openExtensionSetting",
                "title": "Goto Extension Setting"
            },
            {
                "category": "inputstream",
                "command": "input.stream.inputExplorer.refresh",
                "title": "Refresh",
                "icon": "$(refresh)"
            },
            {
                "category": "inputstream",
                "command": "input.stream.image.search",
                "title": "Image Search",
                "icon": "$(search)"
            },
            {
                "category": "inputstream",
                "command": "input.stream.input.create",
                "title": "Create",
                "icon": "$(add)"
            },
            {
                "category": "inputstream",
                "command": "input.stream.input.update",
                "title": "Update",
                "icon": "$(edit)"
            },
            {
                "category": "inputstream",
                "command": "input.stream.input.open",
                "title": "Open",
                "icon": "$(open)"
            },
            {
                "category": "inputstream",
                "command": "input.stream.input.link",
                "title": "View on https://input.stream",
                "icon": "$(link)"
            },
            {
                "category": "inputstream",
                "command": "input.stream.input.remove",
                "title": "Remove",
                "icon": "$(remove)"
            },
            {
                "category": "inputstream",
                "command": "input.stream.login",
                "title": "Login",
                "icon": "$(log-in)"
            }
        ],
        "keybindings": [
            {
                "command": "input.stream.inputExplorer.refresh",
                "key": "ctrl+shift+cmd+l"
            },
            {
                "command": "workbench.view.extension.inputstream-explorer",
                "key": "ctrl+shift+cmd+i",
                "title": "Reveal input.stream Explorer"
            }
        ],
        "menus": {
            "commandPalette": [
                {
                    "command": "input.stream.openExtensionSetting",
                    "when": "false"
                }
            ],
            "view/title": [
                {
                    "command": "input.stream.login",
                    "when": "view == input.stream.inputExplorer",
                    "group": "navigation"
                },    
                {
                    "command": "input.stream.inputExplorer.refresh",
                    "when": "view == input.stream.inputExplorer",
                    "group": "navigation"
                },
                {
                    "command": "input.stream.input.create",
                    "when": "view == input.stream.inputExplorer",
                    "group": "navigation"
                },
                {
                    "command": "input.stream.image.search",
                    "when": "view == input.stream.inputExplorer",
                    "group": "navigation"
                }
            ],
            "view/item/context": [
                {
                    "command": "input.stream.input.link",
                    "when": "view == input.stream.inputExplorer && viewItem == input",
                    "group": "inline@0"
                },
                {
                    "command": "input.stream.input.update",
                    "when": "view == input.stream.inputExplorer && viewItem == input",
                    "group": "inline@1"
                },
                {
                    "command": "input.stream.input.remove",
                    "when": "view == input.stream.inputExplorer && viewItem == input",
                    "group": "inline@2"
                }
            ]
        },
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "inputstream-explorer",
                    "title": "https://input.stream",
                    "icon": "media/astronaut.svg"
                }
            ]
        },
        "views": {
            "inputstream-explorer": [
                {
                    "id": "input.stream.inputExplorer",
                    "name": "Stream",
                    "icon": "media/astronaut.svg",
                    "contextualTitle": "List"
                },
                {
                    "id": "input.stream.fileExplorer",
                    "name": "Files",
                    "contextualTitle": "Filesystem for the given input"
                }
            ]
        }
    },
    "main": "./out/extension.js",
    "scripts": {
        "vscode:prepublish:webpack": "npm run clean && webpack --mode production",
        "vscode:prepublish": "npm run clean && npm run compile",
        "vscode-package": "vsce package --out out/extension.vsix",
        "install-extension": "npm run vscode-package && code --install-extension out/extension.vsix",
        "uninstall-extension": "code --uninstall-extension stackbuild.vscode-inputstream",
        "webpack": "npm run clean && webpack --mode development",
        "watch": "tsc -watch -p ./",
        "compile": "tsc -p ./",
        "lint": "eslint src --ext ts --ignore-pattern 'src/proto/**/*.ts' --fix",
        "pretest": "npm run compile && npm run lint",
        "test": "node ./out/test/runTest.js",
        "clean": "rimraf out/",
        "genproto": "node ./node_modules/@grpc/proto-loader/build/bin/proto-loader-gen-types.js --defaults --oneofs --verbose --includeComments -O ./src/proto --grpcLib @grpc/grpc-js proto/*.proto"
    },
    "dependencies": {
        "@grpc/grpc-js": "1.1.1",
        "@grpc/proto-loader": "0.6.0-pre17",
        "fs-extra": "9.0.1",
        "google-auth-library": "6.1.0",
        "graceful-fs": "4.2.4",
        "luxon": "1.24.1",
        "mv": "^2.1.1",
        "node-fetch": "2.6.0",
        "protobufjs": "6.10.1",
        "request": "2.88.2",
        "sha256-file": "1.0.0",
        "shiki": "^0.2.6",
        "shiki-themes": "^0.2.6",
        "simple-lightbox": "2.1.0",
        "slash": "3.0.0",
        "tail": "2.0.4",
        "rimraf": "3.0.2",
        "mkdirp": "1.0.4",
        "tmp": "0.2.1",
        "vscode-common": "1.50.0",
        "vscode-extension-telemetry": "^0.1.6"
    },
    "devDependencies": {
        "@octokit/types": "5.2.0",
        "@types/chai-string": "1.4.2",
        "@types/chai": "4.2.12",
        "@types/fs-extra": "9.0.1",
        "@types/glob": "^7.1.1",
        "@types/graceful-fs": "4.1.2",
        "@types/luxon": "1.24.3",
        "@types/mocha": "^7.0.2",
        "@types/mv": "2.1.0",
        "@types/node-fetch": "1.6.9",
        "@types/node": "^13.11.0",
        "@types/request": "2.48.5",
        "@types/sinon": "9.0.4",
        "@types/tail": "2.0.0",
        "@types/mkdirp": "0.5.2",
        "@types/rimraf": "2.0.2",
        "@types/tmp": "0.2.0",
        "@types/vscode": "^1.45.0",
        "@typescript-eslint/eslint-plugin": "^2.30.0",
        "@typescript-eslint/parser": "^2.30.0",
        "chai-string": "1.5.0",
        "chai": "4.2.0",
        "eslint": "^6.8.0",
        "glob": "^7.1.6",
        "mocha": "^7.1.2",
        "sinon": "9.0.2",
        "ts-loader": "^8.0.4",
        "typescript": "^3.8.3",
        "vscode-test": "^1.3.0",
        "webpack-cli": "^3.3.12",
        "webpack": "^4.44.2"
    }
}